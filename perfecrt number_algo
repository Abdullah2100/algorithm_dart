String prefencemb(int e){
int sum=0;
List<int> sumNumber=[];

for (int i=0;i<e;i++){
   if((e/i).runtimeType==int){
      sum+=i;
      sumNumber.add(i);
     }
  }
if(sum==e){
   return "perfect Number";
   }
else if(sum!=e){
  for(int j=0;j<sumNumber.length;j++){
     if(sum-(sumNumber[j]+sumNumber[j])==e){
         return sumNumber[j].toString();
         }
       }
     }
return "neither";

}
